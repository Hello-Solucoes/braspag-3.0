{
    "name": "lucas-malicia/braspag-api",
    "description": "This is a channel manager to deal with transaction of credit card",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Lucas Goiana Malicia",
            "email": "lucas@hellosolucoes.com.br"
        }
    ],
    "require": {
